{
  "ConnectionString": {
    "DataContext": "Server=(LocalDb)\\MSSQLLocalDB;Initial Catalog=Archysoft.Demo;Integrated Security=SSPI;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Debug", "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WritoTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "C:\\inetpub\\logs\\d1.archysoft.com\\log-{Date}.txt",
          "retainedFileCountLimit": 7
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*"
}
